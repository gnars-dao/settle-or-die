{
  "manifest_version": 3,
  "name": "Settle or Die",
  "description": "Settle Warning",
  "version": "1.4",
  "permissions": ["storage"],
  "host_permissions": [
    "*://*.thegraph.com/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Skate or Dao",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
